    "google-auth-library": "^8.7.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "node-schedule": "^2.1.1",
    "supabase": "^1.50.11",
    "twilio": "^4.9.0",
    "winston": "^3.8.2"
