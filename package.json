{
  "name": "car-dealership-voice-ai-survey",
  "version": "1.0.0",
  "description": "Automated customer survey system for car dealership service visits using Twilio and AI",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
